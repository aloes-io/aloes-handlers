{
  "name": "aloes-handlers",
  "version": "1.1.5",
  "description": "Encode / decode MQTT stream from IoT devices to Web browsers.",
  "main": "./dist/index.js",
  "unpkg": "aloes-handlers.min.js",
  "jsdelivr": "aloes-handlers.min.js",
  "scripts": {
    "lint": "npx eslint --ext js .",
    "test:aloes-detector": "mocha --require @babel/register './src/Aloes/test/detector.spec.js'",
    "test:aloes-decoder": "mocha --require @babel/register './src/Aloes/test/decoder.spec.js'",
    "test:aloes-encoder": "mocha --require @babel/register './src/Aloes/test/encoder.spec.js'",
    "test:aloes": "npm run test:aloes-detector; npm run test:aloes-decoder; npm run test:aloes-encoder",
    "test:aloes-light-detector": "mocha --require @babel/register './src/Aloes-Light/test/detector.spec.js'",
    "test:aloes-light-decoder": "mocha --require @babel/register './src/Aloes-Light/test/decoder.spec.js'",
    "test:aloes-light-encoder": "mocha --require @babel/register './src/Aloes-Light/test/encoder.spec.js'",
    "test:aloes-light": "npm run test:aloes-light-detector; npm run test:aloes-light-decoder; npm run test:aloes-light-encoder",
    "test:cayenne-detector": "mocha --require @babel/register ./src/CayenneLPP/test/detector.spec.js",
    "test:cayenne-decoder": "mocha --require @babel/register ./src/CayenneLPP/test/decoder.spec.js",
    "test:cayenne-encoder": "mocha --require @babel/register ./src/CayenneLPP/test/encoder.spec.js",
    "test:cayenne": "npm run test:cayenne-detector; npm run test:cayenne-decoder; npm run test:cayenne-encoder",
    "test:mysensors-detector": "mocha --require @babel/register ./src/MySensors/test/detector.spec.js",
    "test:mysensors-decoder": "mocha --require @babel/register ./src/MySensors/test/decoder.spec.js",
    "test:mysensors-encoder": "mocha --require @babel/register ./src/MySensors/test/encoder.spec.js",
    "test:mysensors": "npm run test:mysensors-detector; npm run test:mysensors-decoder; npm run test:mysensors-encoder",
    "test": "npm run test:aloes; npm run test:aloes-light; npm run test:cayenne; npm run test:mysensors",
    "docs:aloesclient": "jsdoc2md ./src/Aloes/lib/* > ./docs/aloesclient/README.md",
    "docs:aloeslight": "jsdoc2md ./src/Aloes-Light/lib/* > ./docs/aloeslight/README.md",
    "docs:cayennelpp": "jsdoc2md ./src/CayenneLPP/lib/* > ./docs/cayennelpp/README.md",
    "docs:mysensors": "jsdoc2md ./src/MySensors/lib/* > ./docs/mysensors/README.md",
    "docs:main": "jsdoc2md ./src/* > ./docs/api/README.md -c ./jsdoc-conf.json",
    "docs:md": "npm run docs:aloesclient ; npm run docs:aloeslight ; npm run docs:cayennelpp ; npm run docs:mysensors",
    "docs:dev": "npm run docs:md ; cp ./README.md ./docs/. ; vuepress dev docs",
    "docs:build": "npm run docs:md ; cp ./README.md ./docs/. ; vuepress build docs",
    "build-lib": "npx babel src --out-dir dist --ignore './src/**/test/*.spec.js'",
    "build-lib-min": "npx babel ./src --ignore './src/**/test/*.spec.js' --presets minify --out-file ./aloes-handlers.min.js",
    "prePublish": "npm run lint; npm run test; npm run build-lib",
    "example": "nodemon --exec babel-node ./examples/index.js"
  },
  "files": [
    "dist/*",
    "package.json",
    "aloes-handlers.min.js",
    "README.md"
  ],
  "author": "getlarge",
  "keywords": [
    "IoT",
    "MQTT",
    "Interoperability"
  ],
  "license": "ISC",
  "dependencies": {
    "lodash.floor": "^4.0.4",
    "lora-packet": "^0.7.7",
    "mqtt-pattern": "^1.2.0",
    "oma-json": "^1.0.5"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0",
    "@babel/core": "^7.3.4",
    "@babel/node": "^7.0.0",
    "@babel/plugin-proposal-class-properties": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.0.0",
    "@babel/plugin-proposal-export-namespace-from": "^7.0.0",
    "@babel/plugin-proposal-function-sent": "^7.0.0",
    "@babel/plugin-proposal-json-strings": "^7.0.0",
    "@babel/plugin-proposal-numeric-separator": "^7.0.0",
    "@babel/plugin-proposal-throw-expressions": "^7.0.0",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0",
    "@babel/plugin-syntax-import-meta": "^7.0.0",
    "@babel/preset-env": "^7.0.0",
    "@babel/register": "^7.0.0",
    "acorn": "^6.1.0",
    "async-mqtt": "^2.0.0",
    "babel-eslint": "^8.2.6",
    "babel-preset-minify": "^0.5.0",
    "chai": "^4.2.0",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^11.3.0",
    "eslint-config-prettier": "^3.1.0",
    "eslint-plugin-babel": "^5.3.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-prettier": "^2.6.2",
    "identity-obj-proxy": "^3.0.0",
    "jsdoc-to-markdown": "^4.0.1",
    "mocha": "^5.2.0",
    "mosca": "^2.8.3",
    "nodemon": "^1.18.8",
    "prettier": "1.16.1",
    "vuepress": "^0.14.9"
  },
  "peerDependencies": {
    "lodash.floor": "^4.0.4",
    "lora-packet": "^0.7.7",
    "mqtt-pattern": "^1.2.0",
    "oma-json": "^1.0.5"
  },
  "mocha.requires": [
    "@babel/register"
  ],
  "babel": {
    "presets": [
      "@babel/preset-env"
    ],
    "plugins": [
      "@babel/plugin-syntax-dynamic-import",
      "@babel/plugin-syntax-import-meta",
      "@babel/plugin-proposal-class-properties",
      "@babel/plugin-proposal-json-strings",
      [
        "@babel/plugin-proposal-decorators",
        {
          "legacy": true
        }
      ],
      "@babel/plugin-proposal-function-sent",
      "@babel/plugin-proposal-export-namespace-from",
      "@babel/plugin-proposal-numeric-separator",
      "@babel/plugin-proposal-throw-expressions"
    ]
  },
  "prettier": {
    "bracketSpacing": false,
    "singleQuote": true,
    "printWidth": 80,
    "trailingComma": "all"
  },
  "eslintConfig": {
    "root": true,
    "env": {
      "es6": true,
      "node": true
    },
    "extends": [
      "airbnb-base",
      "prettier"
    ],
    "plugins": [
      "babel"
    ],
    "parserOptions": {
      "ecmaVersion": 2018,
      "parser": "babel-eslint"
    },
    "rules": {
      "import/no-unresolved": "off",
      "import/extensions": "off",
      "import/prefer-default-export": "off",
      "no-param-reassign": "off",
      "one-var-declaration-per-line": "off"
    },
    "globals": {
      "require": true
    }
  },
  "eslintIgnore": [
    "/dist/",
    "/jsdoc/",
    "/node_modules/",
    "/src/**/test/*.spec.js",
    "aloes-handlers.min.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://framagit.org/aloes/aloes-handlers"
  }
}
